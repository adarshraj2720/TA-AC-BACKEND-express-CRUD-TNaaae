<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
    <h1>USERS</h1>
    <!-- <%= userlist%> -->

    <%userlist.forEach((user)=>{%>
        <p><%=user.name%></p>
        <p><%=user.email%></p>
        <p><%=user.age%></p>
        <p><%=user.bio%></p>
        <a href="/user/<%= user._id%>">
            <button>MORE</button>
        </a>
   <% })%>
    
</body>
</html>